<category><pattern>que companheros tienen el pelo *</pattern>
<template>
	<think>
		<set var="contador"></set>
		<set var="salida"></set>
	</think>
	<condition var = "contador">
		<li>
			<value></value>
		</li>
		<li>
			<value><map name="pelo"><star/></map></value>
			<set var="salida"><get var="salida"/>, <get var="contador"/></set>
			<set var="contador"></set>
			<loop/>
		</li>
		<li>
			<set var="contador"></set>
			<loop/>
		</li>
	</condition>

</template>
</category>

<category><pattern>test *</pattern>
<template><map name = "pelo"><star/></map></template>
</category>


</aiml>
